{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=20160"
          }
        ]
      }
    ],
    "cleanUrls": true
  }
}
