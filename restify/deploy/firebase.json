{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "hosting",
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=1209600"
          }
        ]
      }
    ],
    "cleanUrls": true
  }
}
